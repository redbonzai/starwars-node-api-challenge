{
  "name": "core10-swapi",
  "version": "1.0.0",
  "description": "Star Wars API implementation",
  "author": "Christian Dominguez",
  "license": "ISC",
  "main": "server.ts",
  "nyc": {
    "silent": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "interfaces/**"
    ],
    "reporter": [
      "lcov",
      "text-lcov"
    ]
  },
  "module": "true",
  "scripts": {
    "start-server": "tsc && node build/server.js || exit 1",
    "start": "nodemon -e ts -w ./ -x npm run start-server || exit 1",
    "test": "nyc --silent jasmine-ts --config=./jasmine.json",
    "model-generate": "sequelize-cli model:generate --name "
  },
  "repository": {
    "type": "git",
    "url": "node-ts-starwars"
  },
  "dependencies": {
    "axios": "^1.2.6",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.1",
    "@types/morgan": "^1.9.3",
    "@types/moxios": "^0.4.15",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.5",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "jasmine": "^3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.15",
    "nyc": "^14.1.1",
    "sequelize-cli": "^6.6.0",
    "sinon": "^15.0.1",
    "sqlite3": "^5.1.4",
    "ts-node": "^3.3.0",
    "typescript": "^4.6.3"
  }
}
